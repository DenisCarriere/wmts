{
  "name": "wmts",
  "version": "1.5.2",
  "description": "Flexible WMTS scheme for Javascript applications",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "wmts.js",
    "wmts.min.js"
  ],
  "scripts": {
    "prepublish": "npm run test && npm run build",
    "test": "npm run lint && jest --coverage",
    "lint": "standard",
    "docs": "documentation readme index.js --section=API",
    "build": "browserify index.js -s wmts > wmts.js && uglifyjs wmts.js -c -m > wmts.min.js"
  },
  "author": "Denis Carriere <carriere.denis@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DenisCarriere/wmts/issues"
  },
  "homepage": "https://github.com/DenisCarriere/wmts#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "lodash.range": "^3.2.0",
    "xml-js": "^1.0.2"
  },
  "devDependencies": {
    "browserify": "^14.0.0",
    "coveralls": "^2.11.14",
    "documentation": "^4.0.0-beta.18",
    "jest-cli": "^18.1.0",
    "standard": "^8.6.0",
    "typescript": "^2.1.5",
    "uglify-js": "git://github.com/mishoo/UglifyJS2.git#harmony"
  },
  "keywords": [
    "gis",
    "geo",
    "geojs",
    "geospatial",
    "geography",
    "map",
    "wmts",
    "ogc",
    "scheme"
  ],
  "jest": {
    "verbose": true,
    "collectCoverage": true
  },
  "testEnvironment": "node",
  "standard": {
    "env": "jest"
  }
}
